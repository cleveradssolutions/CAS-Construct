{
	"$schema": "schema/aces.schema.json",
	"general": {
		"actions": [
			{
				"id": "show-consent-form",
				"scriptName": "ShowConsentForm",
				"highlight": false,
				"isAsync": true,
				"params": [
					{
						"id": "if-required-only",
						"type": "boolean",
						"initialValue": "false"
					}
				]
			},
			{
				"id": "set-ad-sounds-muted",
				"scriptName": "SetAdSoundsMuted",
				"highlight": false,
				"params": [
					{
						"id": "is-muted",
						"type": "boolean",
						"initialValue": "false",
						"autocompleteId": ""
					}
				]
			}
		],
		"conditions": [
			{
				"id": "on-consent-form-dismissed",
				"scriptName": "OnConsentFormDismissed",
				"highlight": false,
				"isTrigger": true,
				"isInvertible": false,
				"isCompatibleWithTriggers": false
			},
			{
				"id": "on-consent-required",
				"scriptName": "OnConsentRequired",
				"highlight": false,
				"isTrigger": true,
				"isInvertible": false,
				"isCompatibleWithTriggers": false
			},
			{
				"id": "on-consent-not-required",
				"scriptName": "OnConsentNotRequired",
				"highlight": false,
				"isTrigger": true,
				"isInvertible": false,
				"isCompatibleWithTriggers": false
			},
			{
				"id": "is-consent-required",
				"scriptName": "IsConsentRequired",
				"highlight": false,
				"isInvertible": true
			}
		],
		"expressions": [
			{
				"id": "initialization-error",
				"expressionName": "InitializationError",
				"returnType": "string"
			},
			{
				"id": "user-country-code",
				"expressionName": "UserCountryCode",
				"returnType": "string"
			},
			{
				"id": "consent-status",
				"expressionName": "ConsentStatus",
				"returnType": "string"
			},
			{
				"id": "error-message",
				"expressionName": "ErrorMessage",
				"returnType": "string"
			}
		]
