	"banner-ads": {
		"actions": [
			{
				"id": "load-banner-ad",
				"scriptName": "LoadBannerAd",
				"highlight": true,
				"isAsync": true,
				"params": [
					{
						"id": "ad-size",
						"type": "combo",
						"items": [
							"adaptive",
							"inline",
							"smart",
							"leaderboard",
							"standard"
						]
					},
					{
						"id": "max-width",
						"type": "number",
						"initialValue": "0"
					},
					{
						"id": "max-height",
						"type": "number",
						"initialValue": "0"
					}
				]
			},
			{
				"id": "show-banner-ad",
				"scriptName": "ShowBannerAd",
				"highlight": true,
				"params": [
					{
						"id": "position",
						"type": "combo",
						"items": [
							"top-center",
							"top-left",
							"top-right",
							"bottom-center",
							"bottom-left",
							"bottom-right",
							"middle-center",
							"middle-left",
							"middle-right"
						],
						"initialValue": "bottom-center"
					},
					{
						"id": "x-offset",
						"type": "number",
						"initialValue": "0"
					},
					{
						"id": "y-offset",
						"type": "number",
						"initialValue": "0"
					}
				]
			},
			{
				"id": "hide-banner-ad",
				"scriptName": "HideBannerAd",
				"highlight": false
			},
			{
				"id": "destroy-banner-ad",
				"scriptName": "DestroyBannerAd",
				"highlight": false
			}
		],
		"conditions": [
			{
				"id": "on-banner-ad-loaded",
				"scriptName": "OnBannerAdLoaded",
				"highlight": true,
				"isTrigger": true,
				"isInvertible": false,
				"isCompatibleWithTriggers": false
			},
			{
				"id": "on-banner-ad-failed-to-load",
				"scriptName": "OnBannerAdFailedToLoad",
				"highlight": false,
				"isTrigger": true,
				"isInvertible": false,
				"isCompatibleWithTriggers": false
			},
			{
				"id": "on-banner-ad-clicked",
				"scriptName": "OnBannerAdClicked",
				"highlight": false,
				"isTrigger": true,
				"isInvertible": false,
				"isCompatibleWithTriggers": false
			},
			{
				"id": "is-banner-ad-loaded",
				"scriptName": "IsBannerAdLoaded",
				"highlight": false,
				"isInvertible": true
			}
		],
		"expressions": []
	},
